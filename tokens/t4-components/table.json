{
  "table": {
    "cell": {
      "padding": {
        "x": {
          "sm": {
            "$type": "spacing",
            "$value": "{spacing.2}"
          },
          "md": {
            "$type": "spacing",
            "$value": "{spacing.3}"
          },
          "lg": {
            "$type": "spacing",
            "$value": "{spacing.4}"
          }
        },
        "y": {
          "sm": {
            "$type": "spacing",
            "$value": "{spacing.2}"
          },
          "md": {
            "$type": "spacing",
            "$value": "{spacing.3}"
          },
          "lg": {
            "$type": "spacing",
            "$value": "{spacing.4}"
          }
        }
      }
    },
    "row": {
      "selected": {
        "background": {
          "$type": "color",
          "$value": "{color.state.info.background}"
        }
      }
    },
    "pagination": {
      "padding": {
        "$type": "spacing",
        "$value": "{spacing.3}"
      },
      "gap": {
        "$type": "spacing",
        "$value": "{spacing.4}"
      },
      "background": {
        "$type": "color",
        "$value": "{color.surface.level1}"
      },
      "border": {
        "$type": "color",
        "$value": "{color.border.default}"
      }
    }
  }
}
