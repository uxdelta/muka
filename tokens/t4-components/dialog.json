{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "dialog": {
    "color": {
      "backdrop": {
        "$type": "color",
        "$value": "{color.surface.backdrop}"
      },
      "background": {
        "$type": "color",
        "$value": "{color.surface.level3}"
      },
      "body-background": {
        "$type": "color",
        "$value": "{color.surface.level2}"
      }
    },
    "border": {
      "radius": {
        "$type": "borderRadius",
        "$value": "{border.radius.xl}"
      }
    },
    "shadow": {
      "default": {
        "$type": "boxShadow",
        "$value": "{shadow.600}"
      }
    },
    "width": {
      "sm": {
        "$type": "dimension",
        "$value": "35rem"
      },
      "md": {
        "$type": "dimension",
        "$value": "40rem"
      },
      "lg": {
        "$type": "dimension",
        "$value": "54.5rem"
      },
      "xl": {
        "$type": "dimension",
        "$value": "76.5rem"
      }
    },
    "height": {
      "sm": {
        "$type": "dimension",
        "$value": "25rem"
      },
      "md": {
        "$type": "dimension",
        "$value": "40rem"
      },
      "lg": {
        "$type": "dimension",
        "$value": "45rem"
      },
      "xl": {
        "$type": "dimension",
        "$value": "50rem"
      }
    }
  }
}