{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "alert": {
    "color": {
      "info": {
        "background": {
          "$type": "color",
          "$value": "{color.state.info.background}"
        },
        "foreground": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.state.info.default}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.state.info.default}"
        }
      },
      "success": {
        "background": {
          "$type": "color",
          "$value": "{color.state.success.background}"
        },
        "foreground": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.state.success.default}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.state.success.default}"
        }
      },
      "warning": {
        "background": {
          "$type": "color",
          "$value": "{color.surface.level1}"
        },
        "foreground": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.border.contrast}"
        }
      },
      "error": {
        "background": {
          "$type": "color",
          "$value": "{color.state.error.background}"
        },
        "foreground": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.state.error.default}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.state.error.default}"
        }
      }
    },
    "padding": {
      "sm": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.3}"
        },
        "y": {
          "$type": "dimension",
          "$value": "{spacing.2}"
        }
      },
      "md": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.4}"
        },
        "y": {
          "$type": "dimension",
          "$value": "{spacing.3}"
        }
      },
      "lg": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.5}"
        },
        "y": {
          "$type": "dimension",
          "$value": "{spacing.4}"
        }
      }
    },
    "radius": {
      "$type": "borderRadius",
      "$value": "{alias.radius.md}"
    },
    "gap": {
      "$type": "dimension",
      "$value": "{spacing.3}"
    },
    "icon": {
      "size": {
        "$type": "dimension",
        "$value": "{size.md}"
      }
    },
    "border": {
      "width": {
        "$type": "borderWidth",
        "$value": "{border.width.regular}"
      }
    }
  }
}
