{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "alias": {
    "font": {
      "brand": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Helvetica"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "medium": {
            "$type": "fontWeights",
            "$value": "500"
          },
          "semibold": {
            "$type": "fontWeights",
            "$value": "600"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
      "plain": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Helvetica"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "medium": {
            "$type": "fontWeights",
            "$value": "500"
          },
          "semibold": {
            "$type": "fontWeights",
            "$value": "600"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
      "mono": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Lucida Console"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
    "script": {
      "family": {
        "$type": "fontFamilies",
        "$value": "Times New Roman"
      }
    }
  },
  "alias": {
    "color": {
      "brand": {
      "primary": {
        "$type": "color",
        "$value": "{color.slate.11}"
      },
      "complementary": {
        "$type": "color",
        "$value": "{color.slate.12}"
      },
      "default": {
        "$type": "color",
        "$value": "{color.slate.9}"
      },
      "contrast": {
        "$type": "color",
        "$value": "{color.slate.12}"
      },
      "muted": {
        "$type": "color",
        "$value": "{color.slate.5}"
      }
    },
    "accent": {
      "default": {
        "$type": "color",
        "$value": "{color.slate.9}"
      },
      "hover": {
        "$type": "color",
        "$value": "{color.slate.10}"
      },
      "pressed": {
        "$type": "color",
        "$value": "{color.slate.11}"
      },
      "contrast": {
        "$type": "color",
        "$value": "{color.slate.12}"
      },
      "muted": {
        "$type": "color",
        "$value": "{color.slate.8}"
      },
      "inverse": {
        "default": {
          "$type": "color",
          "$value": "{color.slate.1}"
        },
        "hover": {
          "$type": "color",
          "$value": "{color.slate.2}"
        },
        "pressed": {
          "$type": "color",
          "$value": "{color.slate.3}"
        },
        "contrast": {
          "$type": "color",
          "$value": "{color.slate.1}"
        },
        "muted": {
          "$type": "color",
          "$value": "{color.slate.2}"
        }
      }
    }
  },
  "button": {
    "typography": {
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.sm}",
          "lineHeight": "{font.size.sm}"
        }
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.md}",
          "lineHeight": "{font.size.md}"
        }
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.lg}",
          "lineHeight": "{font.size.lg}"
        }
      }
    }
  }
    }
  }
}