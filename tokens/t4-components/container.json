{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "container": {
    "max-width": {
      "small": {
        "$type": "dimension",
        "$value": "{container.max-width.sm}"
      },
      "medium": {
        "$type": "dimension",
        "$value": "{container.max-width.md}"
      },
      "large": {
        "$type": "dimension",
        "$value": "{container.max-width.lg}"
      },
      "xlarge": {
        "$type": "dimension",
        "$value": "{container.max-width.xl}"
      }
    },
    "gap": {
      "compact": {
        "$type": "spacing",
        "$value": "{size.xl}"
      },
      "default": {
        "$type": "spacing",
        "$value": "{size.2xl}"
      },
      "spacious": {
        "$type": "spacing",
        "$value": "{size.3xl}"
      }
    }
  }
}
