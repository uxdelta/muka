{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "separator": {
    "color": {
      "default": {
        "$type": "color",
        "$value": "{color.border.default}"
      },
      "contrast": {
        "$type": "color",
        "$value": "{color.border.contrast}"
      }
    },
    "width": {
      "$type": "borderWidth",
      "$value": "{size.4xs}"
    },
    "spacing": {
      "sm": {
        "$type": "dimension",
        "$value": "{spacing.2}"
      },
      "md": {
        "$type": "dimension",
        "$value": "{spacing.4}"
      },
      "lg": {
        "$type": "dimension",
        "$value": "{spacing.6}"
      }
    }
  }
}
