{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "alias": {
    "font": {
      "brand": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Helvetica"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "medium": {
            "$type": "fontWeights",
            "$value": "500"
          },
          "semibold": {
            "$type": "fontWeights",
            "$value": "600"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
      "plain": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Helvetica"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "medium": {
            "$type": "fontWeights",
            "$value": "500"
          },
          "semibold": {
            "$type": "fontWeights",
            "$value": "600"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
      "mono": {
        "family": {
          "$type": "fontFamilies",
          "$value": "Lucida Console"
        },
        "weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "400"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "700"
          }
        }
      },
    "script": {
      "family": {
        "$type": "fontFamilies",
        "$value": "Times New Roman"
      }
    }
  },
  "button": {
    "typography": {
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.sm}",
          "lineHeight": "{font.size.sm}"
        }
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.md}",
          "lineHeight": "{font.size.md}"
        }
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{alias.font.brand.family}",
          "fontWeight": "{alias.font.brand.weight.semibold}",
          "fontSize": "{font.size.lg}",
          "lineHeight": "{font.size.lg}"
        }
      }
    }
  }
}
}