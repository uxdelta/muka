{
  "name": "muka-ui",
  "version": "0.1.0",
  "description": "Muka Design System - Multi-brand design tokens and React components",
  "main": "dist/cjs/components/index.js",
  "module": "dist/esm/components/index.js",
  "types": "dist/types/components/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/esm/components/index.js",
      "require": "./dist/cjs/components/index.js"
    },
    "./styles": "./dist/styles/index.css",
    "./styles/*": "./dist/styles/*"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "bin": {
    "muka-ui": "./cli/bin/muka-ui.js"
  },
  "files": [
    "dist",
    "cli"
  ],
  "scripts": {
    "build": "npm run build:tokens && npm run build:components && npm run build:styles",
    "build:tokens": "node scripts/build-tokens.js",
    "build:components": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:styles": "node scripts/build-styles.js",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:visual": "vitest run --config vitest.browser.config.ts",
    "test:visual:update": "vitest run --config vitest.browser.config.ts -u",
    "test:stories": "vitest run --config vitest.stories.config.ts",
    "test:stories:watch": "vitest --config vitest.stories.config.ts",
    "test:stories:ui": "vitest --config vitest.stories.config.ts --ui",
    "test:all": "npm run test:unit && npm run test:visual && npm run test:stories",
    "test:ui": "vitest --ui",
    "figma:connect": "figma-code-connect",
    "figma:connect:dev": "figma-code-connect --dev",
    "figma:connect:publish": "figma-code-connect --publish",
    "lint": "eslint . --ext .ts,.tsx",
    "validate:tokens": "node scripts/validate-tokens.js",
    "setup:automation": "node scripts/setup-automation.js",
    "disable:automation": "node scripts/disable-automation.js",
    "enable:automation": "node scripts/enable-automation.js",
    "check:automation": "node scripts/check-automation-status.js",
    "diagnose:deployment": "node scripts/diagnose-deployment.js",
    "check:deployment": "node scripts/check-deployment-status.js",
    "deploy:manual": "node scripts/manual-deploy.js",
    "deploy:gh-pages": "node scripts/deploy-to-gh-pages.js",
    "postinstall": "husky install",
    "prepare": "husky"
  },
  "keywords": [
    "design-system",
    "design-tokens",
    "react",
    "multi-brand",
    "ai-design-system"
  ],
  "author": "Revi KÃ¶rnmann",
  "license": "MIT",
  "dependencies": {
    "@remixicon/react": "^4.9.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@figma/code-connect": "^1.3.8",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-designs": "^11.1.2",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.5",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.18",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "playwright": "^1.56.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "storybook": "^10.2.8",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uxdelta/muka.git"
  },
  "bugs": {
    "url": "https://github.com/uxdelta/muka/issues"
  },
  "homepage": "https://github.com/uxdelta/muka#readme"
}
