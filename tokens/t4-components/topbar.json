      "color": {
        "$type": "color",
        "$value": "{color.border.default}"
      },
      "padding-y": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "radius": {
        "$type": "borderRadius",
        "$value": "{border.radius.full}"
      }
    }
