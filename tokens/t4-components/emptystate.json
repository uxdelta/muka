{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "emptystate": {
    "container": {
      "padding": {
        "sm": {
          "$type": "dimension",
          "$value": "{spacing.6}"
        },
        "md": {
          "$type": "dimension",
          "$value": "{spacing.8}"
        },
        "lg": {
          "$type": "dimension",
          "$value": "{spacing.12}"
        }
      },
      "gap": {
        "$type": "dimension",
        "$value": "{spacing.4}"
      },
      "maxWidth": {
        "$type": "dimension",
        "$value": "24rem"
      }
    },
    "icon": {
      "size": {
        "sm": {
          "$type": "dimension",
          "$value": "{spacing.8}"
        },
        "md": {
          "$type": "dimension",
          "$value": "{spacing.12}"
        },
        "lg": {
          "$type": "dimension",
          "$value": "{spacing.16}"
        }
      },
      "color": {
        "$type": "color",
        "$value": "{color.text.muted.default}"
      }
    },
    "title": {
      "color": {
        "$type": "color",
        "$value": "{color.text.default.default}"
      }
    },
    "description": {
      "color": {
        "$type": "color",
        "$value": "{color.text.muted.default}"
      }
    },
    "actions": {
      "gap": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "marginTop": {
        "$type": "dimension",
        "$value": "{spacing.4}"
      }
    }
  }
}
