{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "bottombar": {
    "color": {
      "background": {
        "$type": "color",
        "$value": "{color.surface.level3}"
      },
      "border": {
        "$type": "color",
        "$value": "{color.border.default}"
      },
      "tab": {
        "active": {
          "$type": "color",
          "$value": "{color.text.default.default}"
        },
        "inactive": {
          "$type": "color",
          "$value": "{color.text.subtle.default}"
        }
      }
    },
    "floating": {
      "radius": {
        "$type": "borderRadius",
        "$value": "{border.radius.xl}"
      },
      "padding": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "margin-x": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "shadow": {
        "$type": "boxShadow",
        "$value": "{shadow.200}"
      }
    },
    "tab": {
      "icon-size": {
        "$type": "dimension",
        "$value": "24px"
      },
      "gap": {
        "$type": "dimension",
        "$value": "{spacing.1}"
      },
      "padding-y": {
        "$type": "dimension",
        "$value": "{spacing.2}"
      }
    },
    "actions": {
      "padding-t": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "padding-x": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "gap": {
        "$type": "dimension",
        "$value": "{spacing.3}"
      },
      "button-height": {
        "$type": "dimension",
        "$value": "48px"
      }
    }
  }
}
