{
  "$schema": "https://design-tokens.org/schemas/1.0/schema.json",
  "shadow": {
      "none": { "$type": "other", "$value": "none" },
      "xs": { "$type": "other", "$value": "0 1px 2px 0 rgba(0, 0, 0, 0.05)" },
      "sm": { "$type": "other", "$value": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)" },
      "base": { "$type": "other", "$value": "0 1px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)" },
      "md": { "$type": "other", "$value": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)" },
      "lg": { "$type": "other", "$value": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)" },
      "xl": { "$type": "other", "$value": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)" },
      "2xl": { "$type": "other", "$value": "0 25px 50px -12px rgba(0, 0, 0, 0.25)" },
      "inner": { "$type": "other", "$value": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)" }
  }
}