{
  "$schema": {
    "$type": "other",
    "$value": "https://design-tokens.org/schemas/1.0/schema.json"
  },
  "section": {
    "padding": {
      "vertical": {
        "default": {
          "$type": "spacing",
          "$value": "{size.3xl}"
        },
        "compact": {
          "$type": "spacing",
          "$value": "{size.2xl}"
        },
        "spacious": {
          "$type": "spacing",
          "$value": "{size.4xl}"
        }
      },
      "horizontal": {
        "default": {
          "$type": "spacing",
          "$value": "{size.xl}"
        },
        "compact": {
          "$type": "spacing",
          "$value": "{size.md}"
        },
        "spacious": {
          "$type": "spacing",
          "$value": "{size.2xl}"
        }
      }
    }
  },
  "container": {
    "gap": {
      "default": {
        "$type": "spacing",
        "$value": "{size.2xl}"
      },
      "compact": {
        "$type": "spacing",
        "$value": "{size.xl}"
      },
      "spacious": {
        "$type": "spacing",
        "$value": "{size.3xl}"
      }
    },
    "max-width": {
      "small": {
        "$type": "sizing",
        "$value": "640px"
      },
      "medium": {
        "$type": "sizing",
        "$value": "960px"
      },
      "large": {
        "$type": "sizing",
        "$value": "1024px"
      },
      "xlarge": {
        "$type": "sizing",
        "$value": "100%"
      }
    }
  }
}